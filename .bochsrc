boot: cdrom

ata0: enabled=1, ioaddr1=0x1f0, ioaddr2=0x3f0, irq=14
ata0-master: type=cdrom, path=kernel.iso, status=inserted
ata0-slave: type=disk, path=hd.img, mode=flat

memory: guest=32, host=32

# From the docbook:
#   The 'realtime' method sacrifices reproducibility to preserve performance
#   and host-time correlation.
clock: sync=realtime

magic_break: enabled=1
